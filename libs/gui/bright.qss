/****************************************************************************
QSplitter
****************************************************************************/
	QSplitter
	{
	}
	QSplitter::handle
	{
		border-radius: 0px;
		width: 0px;
		height: 0px;
		margin: 2px;
	}
/****************************************************************************
QSizeGrip
****************************************************************************/
	QSizeGrip
	{
		background: transparent;
		image: url(:/images/sizeGrip.png);
		width: 16px;
		height: 16px;
	}
/****************************************************************************
Disabled Inputs
****************************************************************************/
	QLabel:disabled,
	QCheckBox:disabled,
	QLineEdit:disabled,
	QComboBox:disabled,
	QSpinBox:disabled,
	QDoubleSpinBox:disabled,
	QPushButton:disabled
	{
		color: rgba(75, 75, 75, 75);
	}
	QLineEdit:disabled,
	QComboBox:disabled,
	QSpinBox:disabled,
	QDoubleSpinBox:disabled
	{
		border: 1px solid rgba(205, 205, 205, 255);
		background-color: rgba(235, 235, 235, 255);
	}

/****************************************************************************
Specific / Custom Widgets
****************************************************************************/

	QWidget[qssClass="jobWidget"]
	{
		background-color: rgba(235, 235, 235, 255);
		min-height:  80px;
		border-radius: 4px;
	}
	QWidget[qssClass="jobWidget"] *
	{
		padding: 0px;
	}
	QWidget[qssClass="jobWidget"] .QWidget,
	QWidget[qssClass="jobWidget"] QLabel,
	QWidget[qssClass="jobWidget"] QToolButton:!hover
	{
		background-color: transparent;
	}
	QWidget[qssClass="jobWidget"] QLabel[qssClass="titleLabel"]
	{
		font-weight: bold;
	}

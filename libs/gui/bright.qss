/****************************************************************************
General
****************************************************************************/
	QWidget
	{
		background-color: rgba(255, 255, 255, 255);
	}
	QWidget#page, page_2
	{
		background: transparent;
	}
	QToolBar
	{
		color: rgba(25, 25, 25, 255);
		border-top: 1px solid rgba(255, 255, 255, 255);
		border-bottom: 1px solid rgba(205, 205, 205, 255);
	}
	QFrame, QStackedWidget
	{
		background: none;
	}
	QMainWindow
	{
		background: none;
	}
	QTableView,
	QTableWidget,
	QTreeView
	{
		background-color: rgba(255, 255, 255, 255);
	}
	QHeaderView,
	QMdiArea::background
	{
		background-color: rgba(215, 215, 215, 255);
	}
	QTableCornerButton::section,
	QHeaderView::section
	{
		border-top: none;
		border-left: none;
		border-right: 1px solid rgba(255, 255, 255, 255);
		border-bottom: 1px solid rgba(255, 255, 255, 255);
		background-color: rgba(215, 215, 215, 255);
	}
	QTableView::item,
	QTableWidget::item,
	QTreeView::item
	{
		background-color: rgba(255, 255, 255, 255);
	}
	QLineEdit,
	QSpinBox,
	QDoubleSpinBox,
	QProgressBar,
	QComboBox,
	QTabBar::tab
	{
		background: qlineargradient(spread:pad,
			x1:0, y1:0,
			x2:0, y2:1,
			stop:0 rgba(235, 235, 235, 255),
			stop:0.617 rgba(216, 216, 216, 255),
			stop:0.618 rgba(216, 216, 216, 255),
			stop:1 rgba(205, 205, 205, 255)
		);
	}
	QLineEdit:hover,
	QSpinBox:hover,
	QDoubleSpinBox:hover,
	QComboBox:hover,
	.QDateTimeEdit:hover,
	QTabBar::tab:hover
	{
		background: qlineargradient(spread:pad,
			x1:0, y1:0,
			x2:0, y2:1,
			stop:0 rgba(229, 229, 229, 255),
			stop:0.617 rgba(210, 210, 210, 255),
			stop:0.618 rgba(210, 210, 210, 255),
			stop:1 rgba(199, 199, 199, 255)
		);
	}
	QComboBox,
	QDockWidget::title,
	QDoubleSpinBox,
	QLineEdit,
	QListWidget,
	QProgressBar,
	QSlider::groove:horizontal,
	QSpinBox,
	QTabBar::tab,
	QWidget[qssClass="jobList"]
	{
		border: 1px solid rgba(155, 155, 155, 255);
	}
/****************************************************************************
QToolBar
QStatusBar
QToolButton
****************************************************************************/
	QToolBar,
	QStatusBar
	{
		color: rgba(25, 25, 25, 255);
		spacing: 4px; /* spacing between items in the tool bar */
	}
	QToolButton
	{
		background: transparent;
		border: 0px;
	}
	QToolButton#qt_toolbar_ext_button
	{
		background: qlineargradient(spread:pad,
			x1:0, y1:0,
			x2:0, y2:1,
			stop:0 rgba(195, 195, 195, 255),
			stop:0.617 rgba(170, 170, 170, 255),
			stop:0.618 rgba(170, 170, 170, 255),
			stop:1 rgba(155, 155, 155, 255)
		);
	}
	QToolButton:hover,
	QToolButton#qt_toolbar_ext_button:hover
	{
		background: qlineargradient(spread:pad,
			x1:0, y1:0,
			x2:0, y2:1,
			stop:0 rgba(150, 186, 230, 255),
			stop:0.617 rgba(100, 146, 212, 255),
			stop:0.618 rgba(100, 146, 212, 255),
			stop:1 rgba(70, 122, 201, 255)
		);
	}
	QToolButton:pressed,
	QToolButton#qt_toolbar_ext_button:pressed
	{
		background: qlineargradient(spread:pad,
			x1:0, y1:0,
			x2:0, y2:1,
			stop:0 rgba(70, 122, 201, 255),
			stop:0.381 rgba(100, 146, 212, 255),
			stop:0.382 rgba(100, 146, 212, 255),
			stop:1 rgba(150, 186, 230, 255)
		);
	}
	QToolButton:disabled
	{
	}
	QToolButton:checked,
	QToolButton#qt_toolbar_ext_button:checked
	{
		background: qlineargradient(spread:pad,
			x1:0, y1:0,
			x2:0, y2:1,
			stop:0 rgba(105, 69, 25, 255),
			stop:0.381 rgba(135, 93, 36, 255),
			stop:0.382 rgba(135, 93, 36, 255),
			stop:1 rgba(185, 133, 54, 255)
		);
	}
	QToolButton:flat
	{
	}

	QToolBar::separator
	{
		margin-top: 5px;
		margin-bottom: 5px;
		background-color: rgba(205, 205, 205, 255);
		width: 1px;
	}
/****************************************************************************
QMenuBar
****************************************************************************/
	QMenuBar
	{
		background-color: rgba(255, 255, 255, 255);
	}
	QMenuBar::item
	{
		spacing: 3px; /* spacing between menu bar items */
		padding: 6px 12px;
		background: transparent;
		color: rgba(25, 25, 25, 255);
	}
	QMenuBar::item:selected
	{
		/* when selected using mouse or keyboard */
		background-color: rgba(90, 90, 90, 255);
		color: rgba(255, 168, 0, 255);
	}
	QMenuBar::item:pressed
	{
		background-color: rgba(90, 90, 90, 255);
	}
	QMenuBar::item:disabled
	{
		background-color: rgba(70, 70, 70, 255);
	}
/****************************************************************************
QMenu
****************************************************************************/
	QMenu
	{
		background-color: rgba(255, 255, 255, 255); /* sets background of the menu */
		/*border: 1px solid black;*/
	}
	QMenu::item
	{
		/*background-color: rgba(255, 255, 255, 255);*/ /* do not use 'transparent' here, otherwise checked menu entries with colors don't get properly highlighted! */
		color: rgba(25, 25, 25, 255);
	}
	QMenu::item:selected
	{	/* when user selects item using mouse or keyboard */
		background-color: rgba(90, 90, 90, 255);
		color: rgba(255, 168, 0, 255);
	}
	QMenu::separator
	{
		height: 1px;
		background-color: rgba(205, 205, 205, 255);
	}
	QMenu::item:disabled
	{
		color: rgba(115, 115, 115, 255);
	}
	/*
	QMenu::item:checked     // styling for checked menu entries
	QMenu::item:unchecked
	*/
/****************************************************************************
QPushButton
****************************************************************************/
	QPushButton
	{
		border-style: outset;
		border-top: 1px solid 		rgba(215, 215, 215, 255);
		border-bottom: 1px solid 	rgba(135, 135, 135, 255);
		border-left: 0px solid 		rgba(205, 205, 205, 255);
		border-right: 0px solid		rgba(205, 205, 205, 255);
		/*border-radius: 0.2em;*/
		padding: 0px 6px;
		min-width: 2em;
		min-height: 1.5em;
		color: rgba(25, 25, 25, 255);
		background: qlineargradient(spread:pad,
			x1:0, y1:0,
			x2:0, y2:1,
			stop:0 rgba(195, 195, 195, 255),
			stop:0.617 rgba(170, 170, 170, 255),
			stop:0.618 rgba(170, 170, 170, 255),
			stop:1 rgba(155, 155, 155, 255)
		);
	}
	QPushButton:hover
	{
		border-top: 1px solid 		rgba(170, 206, 250, 255);
		border-bottom: 1px solid 	rgba(50, 102, 181, 255);
		border-left: 0px solid 		rgba(80, 132, 211, 255);
		border-right: 0px solid		rgba(80, 132, 211, 255);
		background: qlineargradient(spread:pad,
			x1:0, y1:0,
			x2:0, y2:1,
			stop:0 rgba(150, 186, 230, 255),
			stop:0.617 rgba(100, 146, 212, 255),
			stop:0.618 rgba(100, 146, 212, 255),
			stop:1 rgba(70, 122, 201, 255)
		);
	}
	QPushButton:pressed
	{
		border-top: 1px solid 		rgba(170, 206, 250, 255);
		border-bottom: 1px solid 	rgba(50, 102, 181, 255);
		border-left: 0px solid 		rgba(80, 132, 211, 255);
		border-right: 0px solid		rgba(80, 132, 211, 255);
		background: qlineargradient(spread:pad,
			x1:0, y1:0,
			x2:0, y2:1,
			stop:0 rgba(70, 122, 201, 255),
			stop:0.381 rgba(100, 146, 212, 255),
			stop:0.382 rgba(100, 146, 212, 255),
			stop:1 rgba(150, 186, 230, 255)
		);
		border-style: inset;
	}
	QPushButton:flat
	{
		border: none; /* no border for a flat push button */
		background: transparent;
	}
/****************************************************************************
QTabWidget
QTabBar
****************************************************************************/
	QTabWidget
	{
	}
	QTabWidget::pane
	{
		/* The tab widget frame */
		/*position: absolute;*/
		top: -1px;
		border: 1px solid rgba(175, 175, 175, 255);
		/*border-radius: 5px;*/
	}
	QTabWidget::tab-bar
	{
		alignment: center;
	}
	QTabBar
	{
		background: none;
	}
	QTabBar::tab
	{
		/* Style the tab using the tab sub-control. Note that it reads QTabBar _not_ QTabWidget */
		color: rgba(25, 25, 25, 255);
		background: qlineargradient(spread:pad,
			x1:0, y1:0,
			x2:0, y2:1,
			stop:0 rgba(235, 235, 235, 255),
			stop:0.617 rgba(228, 228, 228, 255),
			stop:0.618 rgba(228, 228, 228, 255),
			stop:1 rgba(225, 225, 225, 255)
		);
		min-width:36ex;
		padding:1px 4px;
	}
	QTabBar::tab:selected, QTabBar::tab:selected:hover
	{
		border-top: 1px solid rgba(135, 135, 135, 255);
		border-left: 1px solid rgba(135, 135, 135, 255);
		border-right: 1px solid rgba(135, 135, 135, 255);
		background: qlineargradient(spread:pad,
			x1:0, y1:0,
			x2:0, y2:1,
			stop:0 rgba(255, 255, 255, 255),
			stop:0.617 rgba(255, 255, 255, 255),
			stop:0.618 rgba(255, 255, 255, 255),
			stop:1 rgba(255, 255, 255, 255)
		);
	}
	QTabBar::tab:!selected
	{
		margin-top: 3px;
	}
	QTabBar::tab:pressed
	{
		background-color: rgba(135, 135, 135, 255);
	}
	QTabBar::scroller
	{   /* the width of the scroll buttons */
		width: 24px;
	}
	QTabBar QToolButton
	{	/* the scroll buttons are tool buttons */
		color: rgba(255, 168, 0, 255);
		background-color: rgba(215, 215, 215, 255);
	}

		background-color: rgba(215, 215, 215, 255);
		color: rgba(25, 25, 25, 255);
		font: bold;
		titlebar-close-icon: url(:/images/dockwidget-close_bright.svg);
		titlebar-normal-icon: url(:/images/dockwidget-float_bright.svg);
	}
	QDockWidget::title
	{
		background-color: rgba(215, 215, 215, 255);
		vertical-align: center;
		text-align: left;
	}
	QDockWidget::close-button, QDockWidget::float-button
	{	/* keeps title bar "slim", there otherwise seems to be some default padding applied. */
		padding: 0;
	}
/****************************************************************************
QSplitter
****************************************************************************/
	QSplitter
	{
	}
	QSplitter::handle
	{
		border-radius: 0px;
		width: 0px;
		height: 0px;
		margin: 2px;
	}
/****************************************************************************
QSizeGrip
****************************************************************************/
	QSizeGrip
	{
		background: transparent;
		image: url(:/images/sizeGrip.png);
		width: 16px;
		height: 16px;
	}
/****************************************************************************
Disabled Inputs
****************************************************************************/
	QLabel:disabled,
	QCheckBox:disabled,
	QLineEdit:disabled,
	QComboBox:disabled,
	QSpinBox:disabled,
	QDoubleSpinBox:disabled,
	QPushButton:disabled,
	QMenu::item:disabled
	{
		color: rgba(115, 115, 115, 255);
	}
	QLineEdit:disabled,
	QComboBox:disabled,
	QSpinBox:disabled,
	QDoubleSpinBox:disabled
	{
		border: 1px solid rgba(205, 205, 205, 255);
		background-color: rgba(235, 235, 235, 255);
	}

/****************************************************************************
Specific / Custom Widgets
****************************************************************************/

	QWidget[qssClass="jobWidget"]
	{
		background-color: rgba(235, 235, 235, 255);
		min-height:  80px;
		border-radius: 4px;
	}
	QWidget[qssClass="jobWidget"] *
	{
		padding: 0px;
	}
	QWidget[qssClass="jobWidget"] .QWidget,
	QWidget[qssClass="jobWidget"] QLabel,
	QWidget[qssClass="jobWidget"] QScrollArea,
	QWidget[qssClass="jobWidget"] QToolButton:!hover
	{
		background-color: transparent;
	}
	QWidget[qssClass="jobWidget"] QLabel[qssClass="titleLabel"]
	{
		font-weight: bold;
	}

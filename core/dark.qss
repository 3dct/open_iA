	}
	
	QComboBox:on
	{
		background: qlineargradient(spread:pad, 
		x1:0, y1:0, 
		x2:0, y2:1, 
		stop:0 rgba(56, 56, 56, 255), 
		stop:0.617 rgba(37, 37, 37, 255), 
		stop:0.618 rgba(37, 37, 37, 255), 
		stop:1 rgba(26, 26, 26, 255));
	}
	.QDateTimeEdit:pressed 
	{
		background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(30, 30, 30, 255), stop:1 rgba(60, 60, 60, 255)); 
